{
    "name": "thorne/laravel-push-notification",
    "description": "Laravel 5 Package for sending push notifications to Android and iOS devices",
    "keywords": ["apns", "gcm", "push", "notification", "laravel", "laravel 5"],
    "license": "MIT",
    "authors": [
        {
            "name": "Curtis Thorne",
            "email": "dirtbikerdude.91@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.* | 5.1.* | 5.2.* | 5.3.* | 5.4.* | 5.5.* | 5.6.*|5.7.*",
        "sly/notification-pusher": "2.x"
    },
    "autoload": {
        "psr-0": {
            "Thorne\\LaravelPushNotification": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thorne\\LaravelPushNotification\\PushNotificationServiceProvider"
            ],
            "aliases": {
                "PushNotification": "Thorne\\LaravelPushNotification\\PushNotification"
            }
        }
    },
    "minimum-stability": "stable"
}
